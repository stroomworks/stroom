<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:FormPanel ui:field="form" styleName="max">
        <g:FlowPanel styleName="max visualisation-assets-add-dialog">
            <form:FormGroup identity="visualisationAssetsUploadFileTo"
                            label="Upload to">
                <g:Label ui:field="lblPath"/>
            </form:FormGroup>
            <form:FormGroup identity="visualisationAssetsFolderName"
                            label="Folder name"
                            addStyleNames="dock-min">
                <g:TextBox ui:field="txtFolderName" addStyleNames="stroom-control" name="folderName"/>
            </form:FormGroup>
        </g:FlowPanel>
    </g:FormPanel>
</ui:UiBinder>
