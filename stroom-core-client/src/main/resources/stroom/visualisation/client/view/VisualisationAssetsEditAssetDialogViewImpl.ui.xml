<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:form="urn:import:stroom.widget.form.client">
    <g:FormPanel ui:field="form" styleName="max">
        <g:FlowPanel styleName="max visualisation-assets-add-dialog">

            <form:FormGroup identity="visualisationAssetsAssetName"
                            label="Name"
                            addStyleNames="dock-min">
                <g:TextBox ui:field="txtAssetName" addStyleNames="stroom-control" name="assetName"/>
            </form:FormGroup>
        </g:FlowPanel>
    </g:FormPanel>
</ui:UiBinder>
